{
  "name": "cybercity",
  "version": "0.0.2",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "yarn db:generate",
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "tap --node-arg=--require=ts-node/register",
    "start": "NODE_ENV=development tsup src/index.ts --watch --onSuccess \"node dist/index.js serve\"",
    "build": "tsup src/index.ts --format cjs --no-splitting",
    "serve": "tsup src/index.ts --watch --onSuccess \"node dist/index.js serve\"",
    "beat": "tsup src/index.ts --watch --onSuccess \"node dist/index.js beat\"",
    "worker": "tsup src/index.ts --watch --onSuccess \"node dist/index.js worker\"",
    "shell": "tsup src/index.ts --watch --onSuccess \"node dist/index.js shell\"",
    "db:generate": "prisma generate",
    "db:makeMigrations": "prisma migrate dev --create-only --preview-feature",
    "db:migrate": "prisma migrate dev --preview-feature",
    "db:migrate:prod": "prisma migrate deploy --preview-feature",
    "db:seed": "prisma db seed --preview-feature"
  },
  "dependencies": {
    "@prisma/client": "2.29.1",
    "@sinclair/typebox": "^0.20.4",
    "apollo-server-fastify": "^3.1.2",
    "bcrypt": "^5.0.1",
    "bullmq": "^1.40.4",
    "class-validator": "^0.13.1",
    "commander": "^8.1.0",
    "dayjs": "^1.10.6",
    "ejs": "^3.1.6",
    "fastify": "^3.20.2",
    "fastify-caching": "^6.1.0",
    "fastify-compress": "^3.6.0",
    "fastify-cookie": "^5.3.1",
    "fastify-cors": "^6.0.2",
    "fastify-jwt": "^3.0.1",
    "fastify-plugin": "^3.0.0",
    "fastify-request-context": "^2.2.0",
    "fastify-static": "^4.2.3",
    "fastify-websocket": "^3.2.1",
    "graphql": "^15.5.1",
    "graphql-query-complexity": "^0.9.0",
    "graphql-tools": "^8.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "point-of-view": "^4.15.1",
    "rate-limiter-flexible": "^2.2.4",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "socket.io": "^4.1.3",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@anatine/esbuild-decorators": "^0.2.17",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "pino-pretty": "^5.1.3",
    "prettier": "^2.3.2",
    "prisma": "2.29.1",
    "tap": "^15.0.9",
    "tsup": "^4.13.1",
    "typescript": "^4.3.5",
    "winston": "^3.3.3"
  }
}
