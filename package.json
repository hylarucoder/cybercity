{
  "name": "cybercity",
  "version": "0.0.3",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "yarn db:generate",
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "tap --node-arg=--require=ts-node/register",
    "start": "NODE_ENV=development tsup src/index.ts --watch --onSuccess \"node dist/index.js serve\"",
    "build": "tsup src/index.ts --format cjs --no-splitting",
    "serve": "tsup src/index.ts --watch --onSuccess \"node dist/index.js serve\"",
    "beat": "tsup src/index.ts --watch --onSuccess \"node dist/index.js beat\"",
    "worker": "tsup src/index.ts --watch --onSuccess \"node dist/index.js worker\"",
    "shell": "tsup src/index.ts --watch --onSuccess \"node dist/index.js shell\"",
    "db:generate": "prisma generate",
    "db:makeMigrations": "prisma migrate dev --create-only --preview-feature",
    "db:migrate": "prisma migrate dev --preview-feature",
    "db:migrate:prod": "prisma migrate deploy --preview-feature",
    "db:seed": "prisma db seed --preview-feature"
  },
  "dependencies": {
    "@prisma/client": "3.10.0",
    "@sinclair/typebox": "^0.23.4",
    "@swc/core": "^1.2.154",
    "apollo-server-fastify": "^3.6.4",
    "bcrypt": "^5.0.1",
    "bullmq": "^1.76.3",
    "class-validator": "^0.13.2",
    "commander": "^9.0.0",
    "dayjs": "^1.10.8",
    "ejs": "^3.1.6",
    "fastify": "^3.27.4",
    "fastify-caching": "^6.2.0",
    "fastify-compress": "^4.0.1",
    "fastify-cookie": "^5.6.0",
    "fastify-cors": "^6.0.3",
    "fastify-jwt": "^4.1.3",
    "fastify-plugin": "^3.0.1",
    "fastify-request-context": "^2.2.0",
    "fastify-static": "^4.5.0",
    "fastify-websocket": "^4.2.0",
    "graphql": "^16.3.0",
    "graphql-query-complexity": "^0.11.0",
    "graphql-tools": "^8.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "point-of-view": "^5.1.0",
    "rate-limiter-flexible": "^2.3.6",
    "redis": "^4.0.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "socket.io": "^4.4.1",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "pino-pretty": "^7.5.3",
    "prettier": "^2.5.1",
    "prisma": "3.10.0",
    "tap": "^16.0.0",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2",
    "winston": "^3.6.0"
  }
}
