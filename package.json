{
  "name": "davinci",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.16.1",
    "bull": "^3.22.0",
    "cache-manager": "^3.4.3",
    "canvas": "^2.7.0",
    "ejs": "^3.1.6",
    "fastify": "^3.14.1",
    "fastify-caching": "^6.1.0",
    "fastify-compress": "^3.4.2",
    "fastify-cookie": "^5.3.0",
    "fastify-cors": "^5.2.0",
    "fastify-jwt": "^2.4.0",
    "fastify-plugin": "^3.0.0",
    "fastify-request-context": "^2.1.3",
    "fastify-schedule": "^1.0.0",
    "fastify-static": "^4.0.1",
    "fastify-websocket": "^3.1.0",
    "graphql": "^15.5.0",
    "graphql-query-complexity": "^0.8.0",
    "graphql-tools": "^7.0.4",
    "joi": "^17.4.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "point-of-view": "^4.14.0",
    "rate-limiter-flexible": "^2.2.1",
    "redis": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "toad-scheduler": "^1.0.2",
    "under-pressure": "^5.6.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "pino-pretty": "^4.7.1",
    "prettier": "^2.2.1",
    "tap": "^15.0.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
