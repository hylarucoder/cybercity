{
  "name": "cybercity",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "yarn db:generate",
    "prebuild": "rimraf dist",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"tests/**/*.ts\\\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "tap --node-arg=--require=ts-node/register",
    "start": "nodemon --config nodemon.json src/index.ts serve",
    "start:debug": "nodemon --config nodemon.json --inspect-brk src/index.ts",
    "build": "tsc -p tsconfig.json",
    "serve": "node -r ts-node/register src/index.ts serve",
    "beat": "node -r ts-node/register src/index.ts beat",
    "worker": "node -r ts-node/register src/index.ts worker",
    "shell": "node --experimental-repl-await -r ts-node/register src/index.ts shell",
    "db:generate": "prisma generate",
    "db:makeMigrations": "prisma migrate dev --create-only --preview-feature",
    "db:migrate": "prisma migrate dev --preview-feature",
    "db:migrate:prod": "prisma migrate deploy --preview-feature",
    "db:seed": "prisma db seed --preview-feature"
  },
  "dependencies": {
    "@prisma/client": "2.23.0",
    "@sinclair/typebox": "^0.16.7",
    "bcrypt": "^5.0.1",
    "bullmq": "^1.26.5",
    "class-validator": "^0.13.1",
    "commander": "^7.2.0",
    "dayjs": "^1.10.4",
    "ejs": "^3.1.6",
    "fastify": "^3.15.1",
    "fastify-caching": "^6.1.0",
    "fastify-compress": "^3.5.0",
    "fastify-cookie": "^5.3.1",
    "fastify-cors": "^6.0.1",
    "fastify-jwt": "^2.4.0",
    "fastify-plugin": "^3.0.0",
    "fastify-request-context": "^2.1.3",
    "fastify-static": "^4.2.1",
    "fastify-websocket": "^3.2.0",
    "graphql": "^15.5.0",
    "graphql-query-complexity": "^0.8.1",
    "graphql-tools": "^7.0.5",
    "lodash": "^4.17.21",
    "mercurius": "^7.6.1",
    "moment": "^2.29.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "point-of-view": "^4.14.0",
    "rate-limiter-flexible": "^2.2.2",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "socket.io": "^4.1.2",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.7",
    "pino-pretty": "^4.8.0",
    "prettier": "^2.3.0",
    "prisma": "2.23.0",
    "tap": "^15.0.9",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4",
    "winston": "^3.3.3"
  },
  "_moduleAliases": {
    "@/service": "src/service",
    "@/utils": "src/utils",
    "@/plugins": "src/plugins",
    "@/tasks": "src/tasks",
    "@/apps": "src/apps",
    "@/graphql": "src/graphql",
    "@/errors": "src/errors"
  }
}
